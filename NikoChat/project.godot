; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="NikoChat"
run/main_scene="res://main_mobile.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://niko.png"

[autoload]

CharacterSprites="*res://CharacterSprites.gd"
SaveManager="*res://SaveManager.gd"
HistoryManager="*res://HistoryManager.gd"
StatusMessageBus="*res://StatusMessageBus.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=480
window/size/borderless=true
window/size/transparent=true
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/per_pixel_transparency/allowed=true
window/handheld/orientation=1

[filesystem]

import/blender/enabled=false

[rendering]

textures/vram_compression/import_etc2_astc=true
viewport/transparent_background=true
